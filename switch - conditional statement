#include <cs50.h>
#include <stdio.h>

int main(void)

{
    int x = get_int("number: ");
    switch(x)
{
    case 1: 
    printf("One\n");
    break;
    case 2:
    printf("Two\n");
    break;
    case 3:
    printf("Three\n");
    break;
    default:
    printf("Sorry\n");
}
}

Second method

#include <cs50.h>
#include <stdio.h>

int main(void)

{
    int x = get_int("number: ");
    switch(x)
{
    case 5: 
    printf("Five\n");
    
    case 4:
    printf("Four\n");
    
    case 3:
    printf("Three\n");
    
    case 2:
    printf("Two\n");
    
    case 1:
    printf("One\n");
    
    default:
    printf("Blast-off\n");
}
}
